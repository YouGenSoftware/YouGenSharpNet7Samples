    private Northwind_dbo_Orders_Qry_IR_FluentValidator searchValidator { get; set; } = default!;
    private MudForm? searchForm = new MudForm();
    private MudTable<Northwind_dbo_Orders_Qry_IR>? primaryTable = new MudTable<Northwind_dbo_Orders_Qry_IR>();
    private String searchFilterText = "";
    private Int32 selectedRowNumber = -1;
    private Northwind_dbo_Orders_Qry_IR searchModel = new Northwind_dbo_Orders_Qry_IR();
    private Northwind_dbo_Orders_Qry_IR referencesModel = new Northwind_dbo_Orders_Qry_IR();
    private IEnumerable<Northwind_dbo_Orders_Qry_IR>? retrievedRecords;
    private String modelName = "Northwind_dbo_Orders_Qry_IR";
    private Boolean waiting;
    private Boolean FilterFuncAll(Northwind_dbo_Orders_Qry_IR element) => FilterFunc(element, searchFilterText);
    private Boolean FilterFunc(Northwind_dbo_Orders_Qry_IR element, String searchString)
    {
        if (String.IsNullOrWhiteSpace(searchString))
            return true;
        if ($"{ element.OrderID_IR } { element.CustomerID } { element.EmployeeID_IR } { element.OrderDate } { element.RequiredDate } { element.ShippedDate } { element.ShipVia } { element.Freight } { element.ShipName } { element.ShipAddress } { element.ShipCity } { element.ShipRegion } { element.ShipPostalCode } { element.ShipCountry } { element.CompanyName } { element.Address } { element.City } { element.Region } { element.PostalCode } { element.Country }".Contains(searchString))
           return true;
        return false;
    }
    private void RowClickEvent(TableRowClickEventArgs<Northwind_dbo_Orders_Qry_IR> tableRowClickEventArgs)
	{
	}
    private String SelectedRowClassFunc(Northwind_dbo_Orders_Qry_IR element, Int32 rowNumber)
    {
        if (selectedRowNumber == rowNumber)
        {
            selectedRowNumber = -1;
            return String.Empty;
        }
        else if (primaryTable!.SelectedItem != null && primaryTable!.SelectedItem.Equals(element))
        {
            selectedRowNumber = rowNumber;
            return "selected";
        }
        else
        {
            return String.Empty;
        }
    }
    private async Task Search(Boolean retrieveAll = false)
    {
        if (!retrieveAll)
            await searchForm!.Validate();
        if (!retrieveAll && !searchForm!.IsValid)
            return;
        waiting = true;
        retrievedRecords = retrieveAll ? await httpClient!.GetAll() : await httpClient!.GetByBestMatchedQueryPattern(searchModel);
        // retrievedRecords = retrieveAll ? await websocketClient!.GetAll() : await websocketClient!.GetByBestMatchedQueryPattern(searchModel);
        waiting = false;
        StateHasChanged();
    }    
    private void ShowReferencedRecords(Northwind_dbo_Orders_Qry_IR context)
    {
        referencesModel = context;        
    }
    private void ParsePrimaryKeyParameter()
    {
        //parse the primary key into the searchModel properties
    }
    protected override async Task OnInitializedAsync()
    {        
        await base.OnInitializedAsync();
        // await websocketClient!.InitializeAsync();
    }
    protected override async Task OnParametersSetAsync() 
    {
        if (!String.IsNullOrWhiteSpace(PrimaryKey))
        {
            ParsePrimaryKeyParameter();
            await Search();
        }
        await base.OnParametersSetAsync();
    }
